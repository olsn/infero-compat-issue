{
  "compilerOptions": {
    "sourceMap": true,
    "module": "es6",
    "noImplicitAny": false,
    "target": "es6",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "suppressImplicitAnyIndexErrors": true,
    "emitDecoratorMetadata": true,
    "jsx": "react",
    "jsxFactory": "createElement"
  },
  "exclude": [
    "node_modules"
  ]
}